/* pages/query/query.wxss */
page{
  background: #f0eff5;
}
.qHead{
  width: 100%;
  height: 400rpx;
  background: #fff;
}
.advertising{
  width: 100%;
  height: 400rpx;
}
.qPath{
  height: 100rpx;
  /* overflow: hidden; */
}
.pathPitch{
  width: 100%;
  height: 100rpx;
  background: #fff;
  font-size: 28rpx;
}
.pathList{
  width: 100%;
  height: 100rpx;
  font-size: 28rpx;
  color: #fba72f;
  padding: 0 40rpx;
  box-sizing: border-box;
  background: #fff;
}
.jt{
  width: 24rpx;
}
.p10{
  padding-left: 10rpx;
}
.circuit{
  overflow: hidden;
}
.cirList{
  width: 100%;
  height: 210rpx;
  background: #fff;
  margin-top: 10rpx;
  padding: 28rpx 40rpx;
  box-sizing: border-box;
  font-size: 24rpx;
}
.cirL{
  text-align: right;
  padding: 30rpx 30rpx 30rpx 0;
  border-right: 1px solid #dcdcdc;
}
.cTj{
  padding: 10rpx;
  background: #18cc00;
  color: #fff;
  border-radius: 6rpx;
}
.cTime,.cMsg{
  margin: 10rpx 0;
  color: #707070;
}
.cirR{
  padding-left: 30rpx;
}
.cName{
  font-size: 32rpx;
  font-weight: bold;
}
.cScz{
  height: 50rpx;
  line-height: 50rpx;
  border: 1px solid #fba72f;
  border-radius: 6rpx;
}
.cScz text{
  color: #fff;
  padding: 0 10rpx;
  margin-right: 10rpx;
  background: #fba72f;
  height: 50rpx;
  display: inline-block;
}